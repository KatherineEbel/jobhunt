{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && TIMING=1 eslint src/**/*.ts* --fix",
    "start": "node -r esbuild-register ./src/index.ts",
    "test:ci": "jest --detectOpenHandles",
    "test": "jest --detectOpenHandles --watchAll"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "logger": "*",
    "mongoose": "^6.5.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  }
}
