    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.4",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.4.1",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.8.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.3",
    "tsconfig": "*",
    "typescript": "^4.5.3"
