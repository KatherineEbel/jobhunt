    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.2.0",
    "logger": "*",
    "mongoose": "^6.5.0",
    "morgan": "^1.10.0",
    "validator": "^13.7.0",
    "yup": "^0.32.11",
    "lib": "*"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
